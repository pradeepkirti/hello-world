[MonthlyEnrollment] =
total([Subscriber ID Enrollment Count] for_each ([Enrollment Date] by month))



/* Track monthly growth */
[MonthlyGrowth] =
total([Subscriber ID Enrollment Count] for_each ([Enrollment Date] by month))

/* Update index-like values */
