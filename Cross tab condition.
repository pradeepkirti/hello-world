formatDate([Your Date Field], 'yyyy-MM-dd')

to_char([Your Date Field], 'DD-MM-YYYY')



((([TransferNow Activity].[TN Transfer Count] + [TransferNow Activity - BOFA].[TN Transfer Count - BOFA]) Where (([Date TN] Between([Udf_Last 12 Months Start Date];[Udf_Last 12 Months End Date])) And ([Mobile TransferNow Activity] = "MOBILE")))
/(Sum(([TransferNow Activity].[TN Transfer Count] + [TransferNow Activity - BOFA].[TN Transfer Count - BOFA]) Where ([Date TN] Between([Udf_Last 12 Months Start Date];[Udf_Last 12 Months End Date])))) In ([Home ID]))




CASE 
  WHEN ([Mobile TransferNow Activity] = 'MOBILE') THEN
    (SUM([TransferNow Activity].[TN Transfer Count] + [TransferNow Activity - BOFA].[TN Transfer Count - BOFA]) 
    FOR [Date TN] BETWEEN [Udf_Last 12 Months Start Date] AND [Udf_Last 12 Months End Date]) 
    /
    (TOTAL (SUM([TransferNow Activity].[TN Transfer Count] + [TransferNow Activity - BOFA].[TN Transfer Count - BOFA]) 
    FOR [Date TN] BETWEEN [Udf_Last 12 Months Start Date] AND [Udf_Last 12 Months End Date]))
  ELSE NULL 
END





SELECT
  ( ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_MMM_YYYY_NM ),
  ( ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_BGN_DT ),
  ( TO_CHAR(ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_END_DT, 'FMMonth YYYY') ),
  ( ADMIN.VW_CQF_SSA_P2P_HOME.HOME_NM || ' - ' || ADMIN.VW_CQF_SSA_P2P_HOME.SRC_HOME_ID::VARCHAR(15) ),
  ( ADMIN.VW_PERS_TYPE_CTGY.PERS_TYPE_CTGY_NM )

,
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ON_US_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ON_US_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ON_OTH_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ON_OTH_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_CARD_TRN_AMT + ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ACCT_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_CARD_TRN_CNT + ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_ACCT_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.CMPLT_SEND_NA_TRN_MKR_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_NA_TRN_MKR_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_ON_US_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_ON_US_TRN_AMT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_ON_OTH_TRN_CNT),
  SUM(ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.RCVD_ON_OTH_TRN_AMT)
FROM
  ADMIN.VW_CQF_SSA_P2P_HOME INNER JOIN ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY ON (ADMIN.VW_CQF_SSA_P2P_HOME.HOME_ID=ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.HOME_ID)
   INNER JOIN ADMIN.VW_PERS_TYPE_CTGY ON (ADMIN.VW_PERS_TYPE_CTGY.PERS_TYPE_CTGY_ID=ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.PERS_TYPE_CTGY_ID)
   INNER JOIN ADMIN.VW_CQ_SSP2P_RPT_PER_DIM ON (ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_END_DT=ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.MO_END_DT)
  
WHERE
  
   ( ADMIN.VW_CQF_SSA_P2P_HOME.HOME_ID IN (SELECT ADMIN.VW_CQF_SSA_P2P_HOME.HOME_ID FROM ADMIN.VW_CQF_SSA_P2P_HOME WHERE {fn RTRIM(ADMIN.VW_CQF_SSA_P2P_HOME.SRC_HOME_ID)} IN @Prompt('Enter Home ID(s)','A',,Multi,)) )  AND  ( ADMIN.VW_CQF_SSA_MTHLY_P2P_SMRY.HOME_ID IN (SELECT ADMIN.VW_CQF_SSA_P2P_HOME.HOME_ID FROM ADMIN.VW_CQF_SSA_P2P_HOME WHERE {fn RTRIM(ADMIN.VW_CQF_SSA_P2P_HOME.SRC_HOME_ID)} IN @Prompt('Enter Home ID(s)','A',,Multi,)) ) )  AND  
  (
   ( ADMIN.VW_CQF_SSA_P2P_HOME.HOME_NM || ' - ' || ADMIN.VW_CQF_SSA_P2P_HOME.SRC_HOME_ID::VARCHAR(15) )  IN  @Prompt('Select Homes:','A','Client Implementation (Home)\Home Name - ID',Multi,Constrained,Not_Persistent,,User:0)
   AND
   ( ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_TO_RPT_MO_CNT )  <  6
  )
GROUP BY
  ( ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_MMM_YYYY_NM ), 
  ( ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_BGN_DT ), 
  ( TO_CHAR(ADMIN.VW_CQ_SSP2P_RPT_PER_DIM.MO_END_DT, 'FMMonth YYYY') ), 
  ( ADMIN.VW_CQF_SSA_P2P_HOME.HOME_NM || ' - ' || ADMIN.VW_CQF_SSA_P2P_HOME.SRC_HOME_ID::VARCHAR(15) ), 
  ( ADMIN.VW_PERS_TYPE_CTGY.PERS_TYPE_CTGY_NM )


/* @VARIABLE('BOUSER') */ 
